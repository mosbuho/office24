<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.backend.admin.AdminMapper">
    <insert id="insertAdmin" parameterType="com.kh.backend.admin.Admin" useGeneratedKeys="true"
        keyProperty="id"> INSERT INTO admin (id, pw, lv) VALUES (#{id}, #{pw}, 1) </insert>
    <select id="findById" parameterType="String" resultType="com.kh.backend.admin.Admin"> SELECT pw
        FROM admin WHERE id = #{id} </select>
</mapper>